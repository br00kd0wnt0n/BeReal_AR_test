{
  "name": "bereal-ar-billboard",
  "version": "1.0.0",
  "description": "BeReal AR Billboard Prototype - Reveal Reality Campaign",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "vite preview --host 0.0.0.0 --port $PORT --strictPort",
    "serve": "npx serve dist -s -l $PORT --config serve.json",
    "deploy": "npm run build && netlify deploy --prod --dir=dist"
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "qr-scanner": "^1.4.2",
    "three": "^0.155.0"
  },
  "devDependencies": {
    "vite": "^4.5.0"
  },
  "keywords": ["ar", "billboard", "bereal", "prototype", "qr-code"],
  "author": "BeReal Team",
  "license": "MIT"
} 